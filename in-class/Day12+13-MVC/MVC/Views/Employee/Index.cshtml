@{
}

@model IEnumerable<Employee>;
<h1>List of employees</h1>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var m in Model)
        {
            <tr>
                <td>@m.Name</td>
                <td>@m.Salary</td>
            </tr>
        }
    </tbody>
</table>